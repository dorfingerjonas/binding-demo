<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="400.0" prefWidth="600.0" style="-fx-padding: 10;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.htl.demo.Slider">
    <bottom>
        <AnchorPane prefHeight="21.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="previousButton" mnemonicParsing="false" text="Previous" />
                <Button fx:id="nextButton" layoutX="538.0" mnemonicParsing="false" text="Next" />
            </children>
        </AnchorPane>
    </bottom>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="textField" prefHeight="40.0" prefWidth="580.0" />
            <Slider fx:id="horizontalSlider" blockIncrement="1.0" majorTickUnit="1.0" max="5.0" min="1.0" minorTickCount="1" showTickLabels="true" showTickMarks="true" snapToTicks="true" value="1.0">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Slider>
         </children></VBox>
   </center>
   <top>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="SEW NOTE" BorderPane.alignment="CENTER">
         <font>
            <Font size="47.0" />
         </font>
      </Text>
   </top>
</BorderPane>

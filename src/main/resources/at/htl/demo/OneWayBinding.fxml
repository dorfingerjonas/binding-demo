<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="400.0" prefWidth="600.0" style="-fx-padding: 10;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.htl.demo.OneWayBindingController">
    <bottom>
        <AnchorPane prefHeight="21.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" text="Previous" />
                <Button layoutX="538.0" mnemonicParsing="false" text="Next" />
            </children>
        </AnchorPane>
    </bottom>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="label" alignment="TOP_LEFT" prefHeight="80.0" prefWidth="580.0" text="Label">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="showButton" mnemonicParsing="false" text="Button">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
            <TextArea fx:id="textArea" prefHeight="124.0" prefWidth="580.0">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </TextArea>
         </children>
      </VBox>
   </center>
</BorderPane>
